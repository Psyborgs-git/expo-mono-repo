# Environment Configuration
NODE_ENV=development

# API Configuration
EXPO_PUBLIC_GRAPHQL_ENDPOINT=http://localhost:4000/graphql
EXPO_PUBLIC_WEBSOCKET_ENDPOINT=ws://localhost:4000/graphql
EXPO_PUBLIC_API_VERSION=v1

# Development Tools
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true
EXPO_PUBLIC_ENABLE_TEST_NAVIGATION=true

# Analytics (optional)
EXPO_PUBLIC_SENTRY_DSN=
EXPO_PUBLIC_ANALYTICS_KEY=

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_CHAT=true
EXPO_PUBLIC_ENABLE_REAL_TIME_CHAT=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true