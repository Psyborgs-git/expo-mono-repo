{
	"name": "expo-mono-repo",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@8.15.0",
	"workspaces": [
		"apps/*",
		"packages/*",
		"tools"
	],
	"scripts": {
		"bootstrap": "pnpm install",
		"build": "turbo run build",
		"dev": "turbo run dev",
		"test": "turbo run test",
		"test:watch": "turbo run test:watch",
		"test:coverage": "turbo run test:coverage",
		"lint": "turbo run lint",
		"lint:fix": "turbo run lint:fix",
		"type-check": "turbo run type-check",
		"format": "turbo run format",
		"format:check": "turbo run format:check",
		"clean": "turbo run clean && rm -rf node_modules",
		"reset": "pnpm clean && pnpm install",
		"start:carat-central": "pnpm --filter @bdt/carat-central start",
		"start:vloop": "pnpm --filter @bdt/vloop start",
		"start:parallax": "pnpm --filter @bdt/parallax start",
		"create-app": "tsx tools/src/create-app.ts",
		"create-package": "tsx tools/src/create-package.ts",
		"check-deps": "syncpack list-mismatches",
		"fix-deps": "syncpack fix-mismatches",
		"update-deps": "pnpm -r update"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@react-native-community/cli": "latest",
		"@tamagui/babel-plugin": "^1.135.4",
		"eslint": "^8.57.1",
		"prettier": "^3.6.2",
		"syncpack": "^13.0.4",
		"tsx": "^4.7.1",
		"turbo": "^2.5.8",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@apollo/client": "^4.0.7",
		"@react-native-async-storage/async-storage": "^2.2.0",
		"@shopify/react-native-skia": "^2.3.6",
		"@tamagui/config": "^1.135.4",
		"@tamagui/core": "^1.135.4",
		"@tamagui/font-inter": "^1.135.4",
		"@tamagui/metro-plugin": "^1.135.4",
		"@tamagui/themes": "^1.135.4",
		"expo": "^54.0.20",
		"expo-constants": "^18.0.10",
		"expo-font": "^14.0.9",
		"expo-linking": "^8.0.8",
		"expo-router": "^6.0.13",
		"expo-splash-screen": "^31.0.10",
		"expo-status-bar": "^3.0.8",
		"graphql": "^16.11.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-reanimated": "^4.1.3",
		"react-native-safe-area-context": "^5.6.1",
		"react-native-screens": "~4.16.0",
		"react-native-vision-camera": "^4.7.2",
		"react-native-web": "^0.21.2",
		"rxjs": "^7.8.2",
		"tamagui": "^1.135.4"
	}
}
